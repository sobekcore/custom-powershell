{
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": " PowerShell 5.1 > {{.Path}}",
  "final_space": false,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#c8c8dc",
          "background": "#2f3438",
          "properties": {
            "prefix": " \uF490 "
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#100e23",
          "background": "#91ddff",
          "properties": {
            "folder_separator_icon": "\u002F",
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#193549",
          "background": "#95ffa4",
		        "properties": {
			      "branch_icon": "\uE0A0 ",
			      "no_commits_icon": "\uE0A0",
			      "branch_gone_icon": "\u2260",
			      "branch_identical_icon": "\uF0C9",
			      "branch_ahead_icon": "\u2191",
			      "branch_behind_icon": "\u2193",
			      "local_working_icon": "",
			      "local_staged_icon": ""
		      }
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#ff8080",
          "properties": {
            "prefix": " \u0425 "
          }
        }
      ]
    },
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#91ddff",
          "properties": {
            "text": "\u003E"
          }
        }
      ]
    }
  ]
}
